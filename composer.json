{
  "name": "mohamedgaldi/vilt-filepond",
  "description": "A Laravel package for handling file uploads with FilePond in VILT stack",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Galdi",
      "email": "contact.galdi@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "MohamedGaldi\\ViltFilepond\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MohamedGaldi\\ViltFilepond\\ViltFilePondServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "laravel",
    "vilt",
    "filepond",
    "upload",
    "vue",
    "file upload",
    "vilt stack"
  ],
  "support": {
    "email": "contact.galdi@gmail.com",
    "issues": "https://github.com/mohamedgaldi/vilt-filepond/issues",
    "source": "https://github.com/mohamedgaldi/vilt-filepond"
  },
  "homepage": "https://github.com/mohamedgaldi/vilt-filepond"
}
